{
  "name": "loopback-starter",
  "version": "0.0.1",
  "description": "loopback-starter",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t loopback-starter .",
    "docker:run": "docker run -p 3000:3000 -d loopback-starter",
    "premigrate": "npm run build",
    "migrate": "node ./dist/src/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "aman <aman.chhabra@sourcefuse.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.1.1",
    "@loopback/core": "^2.13.0",
    "@loopback/logging": "^0.4.4",
    "@loopback/repository": "^3.2.1",
    "@loopback/rest": "^9.1.0",
    "@loopback/rest-explorer": "^3.0.4",
    "@loopback/service-proxy": "^3.0.4",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-postgresql": "^5.3.0",
    "loopback4-authentication": "^4.0.1",
    "loopback4-authorization": "^3.1.1",
    "loopback4-soft-delete": "^3.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.7",
    "@loopback/eslint-config": "^10.0.3",
    "@loopback/testlab": "^3.2.9",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^10.17.45",
    "eslint": "^7.13.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.5"
  }
}
